{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "generators": {
      "streamelements": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/generated/stream/#{name}",
        "inputSpec": "https://stoplight.io/api/v1/projects/streamelements/kappa/nodes/api.yaml?deref=optimizedBundle",
        "skipValidateSpec": true,
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter":true
        }
      }
    }
  }
}
