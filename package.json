  "dependencies": {
    "axios": "^0.21.4",
    "axios-cache-interceptor": "^0.2.5",
    "dotenv": "^10.0.0",
    "socket.io-client": "^3.1.3",
    "steam-community": "^0.2.2",
    "steam-totp": "^2.1.1",
    "steam-tradeoffer-manager": "^2.10.3",
    "steam-user": "^4.19.12",
    "winston": "^3.3.3"
